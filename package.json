{
  "name": "email-cadence-monorepo",
  "private": true,
  "workspaces": ["apps/*"],
  "scripts": {
    "dev": "concurrently -n WEB,API,WORKER -c auto \"npm run dev:web\" \"npm run dev:api\" \"npm run dev:worker\"",
    "dev:web": "npm --workspace apps/web run dev",
    "dev:api": "npm --workspace apps/api run start:dev",
    "dev:worker": "npm --workspace apps/worker run dev"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "typescript": "^5.5.4"
  }
}
